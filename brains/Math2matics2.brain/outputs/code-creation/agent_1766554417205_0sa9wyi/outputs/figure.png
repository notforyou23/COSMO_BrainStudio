<?xml version="1.0" encoding="UTF-8"?>
<svg xmlns="http://www.w3.org/2000/svg" width="900" height="520" viewBox="0 0 900 520" role="img" aria-label="End-to-end demo figure">
  <defs>
    <style>
      .bg { fill: #ffffff; }
      .grid { stroke: #e6e6e6; stroke-width: 1; shape-rendering: crispEdges; }
      .axis { stroke: #222; stroke-width: 2; shape-rendering: crispEdges; }
      .label { font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Helvetica, Arial, sans-serif; fill: #222; font-size: 14px; }
      .title { font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Helvetica, Arial, sans-serif; fill: #111; font-size: 18px; font-weight: 600; }
      .small { font-family: ui-monospace, SFMono-Regular, Menlo, Monaco, Consolas, "Liberation Mono", "Courier New", monospace; fill: #555; font-size: 12px; }
      .line { fill: none; stroke: #2f6feb; stroke-width: 3; }
      .band { fill: rgba(47,111,235,0.12); stroke: none; }
      .pt { fill: #2f6feb; stroke: #1b4fb8; stroke-width: 1; }
    </style>
  </defs>

  <rect class="bg" x="0" y="0" width="900" height="520"/>

  <text class="title" x="30" y="38">E2E Demo Figure (support artifact)</text>
  <text class="label" x="30" y="62">Synthetic metric trend with uncertainty band (placeholder-free, deterministic SVG stored as figure.png)</text>
  <text class="small" x="30" y="86">Note: Stored as SVG content with .png extension for bootstrap stage; later runs may overwrite with true PNG.</text>

  <g transform="translate(70,110)">
    <rect x="0" y="0" width="790" height="360" fill="#fff" stroke="#ddd"/>

    <!-- grid -->
    <g>
      <line class="grid" x1="0" y1="60" x2="790" y2="60"/>
      <line class="grid" x1="0" y1="120" x2="790" y2="120"/>
      <line class="grid" x1="0" y1="180" x2="790" y2="180"/>
      <line class="grid" x1="0" y1="240" x2="790" y2="240"/>
      <line class="grid" x1="0" y1="300" x2="790" y2="300"/>
      <line class="grid" x1="98.75" y1="0" x2="98.75" y2="360"/>
      <line class="grid" x1="197.5" y1="0" x2="197.5" y2="360"/>
      <line class="grid" x1="296.25" y1="0" x2="296.25" y2="360"/>
      <line class="grid" x1="395" y1="0" x2="395" y2="360"/>
      <line class="grid" x1="493.75" y1="0" x2="493.75" y2="360"/>
      <line class="grid" x1="592.5" y1="0" x2="592.5" y2="360"/>
      <line class="grid" x1="691.25" y1="0" x2="691.25" y2="360"/>
    </g>

    <!-- axes -->
    <line class="axis" x1="0" y1="360" x2="790" y2="360"/>
    <line class="axis" x1="0" y1="0" x2="0" y2="360"/>

    <!-- y labels -->
    <g>
      <text class="label" x="-10" y="365" text-anchor="end">0.0</text>
      <text class="label" x="-10" y="305" text-anchor="end">0.2</text>
      <text class="label" x="-10" y="245" text-anchor="end">0.4</text>
      <text class="label" x="-10" y="185" text-anchor="end">0.6</text>
      <text class="label" x="-10" y="125" text-anchor="end">0.8</text>
      <text class="label" x="-10" y="65" text-anchor="end">1.0</text>
    </g>

    <!-- x labels -->
    <g>
      <text class="label" x="0" y="386" text-anchor="start">t0</text>
      <text class="label" x="197.5" y="386" text-anchor="middle">t2</text>
      <text class="label" x="395" y="386" text-anchor="middle">t4</text>
      <text class="label" x="592.5" y="386" text-anchor="middle">t6</text>
      <text class="label" x="790" y="386" text-anchor="end">t8</text>
      <text class="label" x="395" y="414" text-anchor="middle">time (steps)</text>
    </g>

    <!-- deterministic synthetic series (values in [0,1]) mapped to y: 360*(1-v) -->
    <!-- values: [0.18,0.22,0.30,0.41,0.48,0.57,0.63,0.71,0.78] -->
    <!-- +/- band: 0.06 -->
    <path class="band" d="M 0 316.8
      L 98.75 302.4
      L 197.5 273.6
      L 296.25 234.0
      L 395 208.8
      L 493.75 176.4
      L 592.5 154.8
      L 691.25 126.0
      L 790 100.8
      L 790 144.0
      L 691.25 169.2
      L 592.5 198.0
      L 493.75 219.6
      L 395 252.0
      L 296.25 277.2
      L 197.5 316.8
      L 98.75 345.6
      L 0 360.0
      Z"/>
    <path class="line" d="M 0 295.2
      L 98.75 280.8
      L 197.5 252.0
      L 296.25 212.4
      L 395 187.2
      L 493.75 154.8
      L 592.5 133.2
      L 691.25 104.4
      L 790 79.2"/>

    <circle class="pt" cx="0" cy="295.2" r="4"/>
    <circle class="pt" cx="98.75" cy="280.8" r="4"/>
    <circle class="pt" cx="197.5" cy="252.0" r="4"/>
    <circle class="pt" cx="296.25" cy="212.4" r="4"/>
    <circle class="pt" cx="395" cy="187.2" r="4"/>
    <circle class="pt" cx="493.75" cy="154.8" r="4"/>
    <circle class="pt" cx="592.5" cy="133.2" r="4"/>
    <circle class="pt" cx="691.25" cy="104.4" r="4"/>
    <circle class="pt" cx="790" cy="79.2" r="4"/>

    <text class="label" x="10" y="20">metric</text>
  </g>

  <text class="small" x="30" y="505">Schema note: This artifact will be referenced in outputs/manifest.json by relative path "outputs/figure.png".</text>
</svg>
